{
  "name": "politico-manual-react-scripts",
  "version": "1.0.0",
  "description": "custom webpack config for react politico",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "yarn dev --hot",
    "start": "serve -s dist",
    "heroku-postbuild": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "pretty-quick --staged",
    "lint": "eslint ./",
    "storybook": "start-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn test",
      "pre-push": "yarn deploy-storybook"
    }
  },
  "author": "tevin thuku",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tevinthuku/politico-react-manual-react-config.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Tevinthuku/politico-react-manual-react-config/issues"
  },
  "homepage": "https://github.com/Tevinthuku/politico-react-manual-react-config#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.0.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "9.x",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react-hot-loader": "^4.8.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "serve": "^11.0.0"
  }
}
